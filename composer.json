{
    "name": "juzaweb/multiple-languages",
    "type": "juzaweb-plugin",
    "license": "MIT",
    "homepage": "https://juzaweb.com",
    "keywords": [
        "framework",
        "laravel",
        "laravel cms",
        "laravel cms like wordpress",
        "cms like wordpress",
        "juzaweb",
        "juzacms",
        "juzaweb multiple languages",
        "juzaweb plugin"
    ],
    "description": "Multiple Languages support for Juzaweb CMS",
    "extra": {
        "juzaweb": {
            "name": "Multiple Languages",
            "domain": "mlla",
            "providers": [
                "Juzaweb\\Multilang\\Providers\\MultilangServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Juzaweb\\Multilang\\": "src/"
        }
    },
    "require-dev": {
        "juzaweb/modules": "^1.0",
        "juzaweb/dev-tool": "^1.0"
    }
}
