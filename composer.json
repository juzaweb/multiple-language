{
    "name": "juzaweb/multiple-languages",
    "description": "Multiple Languages support for Juzaweb CMS",
    "extra": {
        "juzaweb": {
            "name": "Multiple Languages",
            "domain": "mlla",
            "providers": [
                "Juzaweb\\Multilang\\Providers\\MultilangServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Juzaweb\\Multilang\\": "src/"
        }
    },
    "require-dev": {
        "juzaweb/modules": "^1.0",
        "juzaweb/dev-tool": "^1.0"
    }
}
